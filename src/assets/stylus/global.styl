.strike {
    position: relative;

    border-bottom: 1px #ddd solid;
    height: 20px;

    margin-bottom: 20px;

    .or {
        position: absolute;
        left: 48%;
        top: 7px;

        padding: 0 3px;

        background-color: #f9f9f9;
    }

}

.btn-numbered-page, .btn-next-page, .btn-prev-page, .btn-first-page, .btn-last-page {
    a {
        cursor: pointer;
    }
}